{
  "account": {
    "aws_account_id": "AWS_ACCOUNT_ID_GOES_HERE",
    "kms_key_alias": "PREFIX_GOES_HERE_streamalert_secrets",
    "prefix": "PREFIX_GOES_HERE",
    "region": "us-east-1"
  },
  "general": {
    "matcher_locations": [
      "matchers"
    ],
    "rule_locations": [
      "rules"
    ]
  },
  "infrastructure": {
    "alerts_table": {
      "read_capacity": 5,
      "write_capacity": 5
    },
    "firehose": {
      "use_prefix": true,
      "buffer_interval": 900,
      "buffer_size": 128,
      "compression_format": "GZIP",
      "enabled": false,
      "enabled_logs": {}
    },
    "monitoring": {
      "create_sns_topic": true
    },
    "rule_staging": {
      "cache_refresh_minutes": 10,
      "enabled": false,
      "table": {
        "read_capacity": 20,
        "write_capacity": 5
      }
    },
    "classifier_sqs": {
      "use_prefix": true
    }
  },
  "terraform": {
    "create_bucket": true,
    "lock_state_file": false,
    "tfstate_bucket": "PREFIX_GOES_HERE-streamalert-terraform-state",
    "tfstate_s3_key": "streamalert_state/terraform.tfstate"
  }
}
