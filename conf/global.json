{
  "account": {
    "aws_account_id": "AWS_ACCOUNT_ID_GOES_HERE",
    "kms_key_alias": "PREFIX_GOES_HERE_streamalert_secrets",
    "prefix": "PREFIX_GOES_HERE",
    "region": "us-east-1"
  },
  "general": {
    "matcher_locations": [
      "matchers"
    ],
    "rule_locations": [
      "rules"
    ]
  },
  "infrastructure": {
    "alerts_table": {
      "read_capacity": 5,
      "write_capacity": 5
    },
    "firehose": {
      "buffer_interval": 900,
      "buffer_size": 128,
      "compression_format": "GZIP",
      "enabled": false,
      "enabled_logs": {}
    },
    "monitoring": {
      "create_sns_topic": true
    },
    "rule_staging": {
      "cache_refresh_minutes": 10,
      "enabled": false,
      "table": {
        "read_capacity": 20,
        "write_capacity": 5
      }
    },
    "classifier_sqs": {
      "use_prefix": null
    }
  },
  "lookup_tables": {
    "buckets": {
      "airbnb.sample.lookuptable": [
        "resource_map_legacy.gz"
      ]
    },
    "cache_refresh_minutes": 10,
    "enabled": false
  },
  "s3_access_logging": {
    "create_bucket": true,
    "logging_bucket": "PREFIX_GOES_HERE.streamalert.s3-logging"
  },
  "terraform": {
    "create_bucket": true,
    "tfstate_bucket": "PREFIX_GOES_HERE.streamalert.terraform.state",
    "tfstate_s3_key": "stream_alert_state/terraform.tfstate"
  }
}
