{
  "id": "prod",
  "modules": {
    "cloudwatch_monitoring": {
      "enabled": true,
      "kinesis_alarms_enabled": false,
      "lambda_alarms_enabled": true
    },
    "stream_alert": {
      "classifier": {
        "enable_metrics": true,
        "log_level": "info",
        "memory": 128,
        "timeout": 10
      },
      "rule_processor": {
        "enable_metrics": true,
        "log_level": "info",
        "memory": 128,
        "timeout": 10
      }
    }
  },
  "region": "us-east-1"
}