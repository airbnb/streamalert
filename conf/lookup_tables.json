{
  "enabled": false,
  "tables": {
    "resource_map_prototype": {
      "driver": "s3",
      "bucket": "airbnb.sample.lookuptable",
      "key": "resource_map.gz",
      "cache_refresh_minutes": 10,
      "compression": "gzip"
    },
    "resource_map_dynamodb": {
      "driver": "dynamodb",
      "table": "some_table_name",
      "partition_key": "MyPartitionKey",
      "value_key": "MyValueKey",
      "cache": {
        "refresh_minutes": 2,
        "maximum_keys": 10
      }
    }
  }
}
